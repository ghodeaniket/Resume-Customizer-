{
  "name": "resume-customizer-backend",
  "version": "1.0.0",
  "description": "Backend for resume customizer application",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:docker": "./dev-start.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.test.js'",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.js'",
    "test:e2e": "jest --testMatch='**/tests/e2e/**/*.test.js'",
    "test:ci": "jest --ci --runInBand --coverage",
    "lint": "eslint .",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down"
  },
  "dependencies": {
    "awilix": "^12.0.5",
    "aws-sdk": "^2.980.0",
    "axios": "^0.24.0",
    "axios-retry": "^4.5.0",
    "bcrypt": "^5.0.1",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-safe": "^9.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^4.6.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^8.5.1",
    "marked": "^9.1.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "pdf-parse": "^1.1.1",
    "pg": "^8.7.1",
    "prom-client": "^14.0.1",
    "puppeteer": "^24.4.0",
    "redis": "^4.7.0",
    "sequelize": "^6.6.5",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.3.4"
  }
}
