    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-safe": "^9.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^4.6.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^8.5.1",
    "marked": "^9.1.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "prom-client": "^14.0.1",
